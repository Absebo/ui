{
  "name": "@buttercup/ui",
  "version": "0.6.1",
  "description": "Buttercup UI Components in React",
  "main": "dist/index.js",
  "repository": "git@github.com:buttercup/buttercup-ui.git",
  "author": "Sallar Kaboli <sallar.kaboli@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublish": "npm run build",
    "format": "prettier --write '{src,stories}/**/*.js'",
    "precommit": "lint-staged",
    "deploy": "./deploy"
  },
  "lint-staged": {
    "{src,stories}/**/*.js": ["prettier --write", "git add"]
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4"
  },
  "dependencies": {
    "@buttercup/generator": "0.0.3",
    "classnames": "^2.2.5",
    "filter-react-dom-props": "^0.0.2",
    "polished": "^1.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-icons": "^2.2.5",
    "react-popover": "^0.4.14",
    "styled-components": "^1.4.4",
    "zxcvbn": "^4.4.2"
  }
