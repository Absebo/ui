{
    "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
            "targets": {
                "chrome": 66
            },
            "useBuiltIns": "usage",
            "loose": true
        }]
    ],
    "plugins": [
        "ramda",
        "@babel/plugin-transform-spread",
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-function-bind",
        "jsx-control-statements"
    ]
}
